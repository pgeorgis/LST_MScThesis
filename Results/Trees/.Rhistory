#LOAD AND PREPARE DATA
#Load (or install, if necessary) the following packages:
library(dplyr)
#VOWEL OVERLAP MEASUREMENT USING PILLAI SCORE
#Script written by Philip Georgis, primarily based on the tutorial by Joey Stanley:
#https://joeystanley.com/blog/a-tutorial-in-calculating-vowel-overlap
#LOAD AND PREPARE DATA
#Load (or install, if necessary) the following packages:
library(dplyr)
library(ggplot2)
library(heplots)
#Load in csv file with vowel formant measurements (Bark scale)
my_vowels_raw <- read.csv("formant_measurements_bark.csv")
#Filter out unnecessary columns from dataset
my_vowels <- my_vowels_raw %>% select(vowel, duration, F1, F2, lang)
#Create a new column called "vowel category", which collapses the distinction between stressed and unstressed vowels
#Note: stressed vowels were annotated using capital letters (e.g. 'A') and unstressed vowels using lowercase (e.g 'a')
my_vowels$vowel_category = tolower(my_vowels$vowel)
#PILLAI SCORES: CALCULATION AND VISUALIZATION
#Calculate Pillai Score using MANOVA Test
pillai <- function(lang1, lang2, vowel_cats, duration = FALSE) {
vowel_cat <- my_vowels %>%
filter(vowel %in% vowel_cats)
lang_vowels <- vowel_cat %>%
filter(lang %in% c(lang1, lang2))
if (duration == TRUE) {
summary(manova(cbind(F1, F2, duration) ~ lang, data = lang_vowels))$stats["lang","Pillai"]
} else {
summary(manova(cbind(F1, F2) ~ lang, data = lang_vowels))$stats["lang","Pillai"]
}
}
#e.g. calculate Pillai score for /o/ vowels (stressed 'O' and unstressed 'o') in Czech and Russian, with duration
pillai('Czech', 'Russian', c('O', 'o'), duration=TRUE)
#e.g. calculate the same Pillai score, but without duration
pillai('Czech', 'Russian', c('O', 'o'), duration=FALSE)
#Plot Pillai heplots for a vowel category in a pair of languages
plot_pillai <- function(lang1, lang2, vowel_cats) {
vowel_cat <- my_vowels %>%
filter(vowel %in% vowel_cats)
lang_vowels <- vowel_cat %>%
filter(lang %in% c(lang1, lang2))
model = manova(cbind(F1, F2) ~ lang, data = lang_vowels)
heplot(model, term.labels=FALSE)
}
#e.g. create a Pillai heplot for the /o/ vowel in Czech and Russian
plot_pillai('Czech', 'Russian', c('o'))
#PLOT VOWEL SPACE OVERLAP WITH ELLIPSES
plot_overlap <- function(langs, vowel_cats) {
vowel_cat <- my_vowels %>%
filter(vowel_category %in% vowel_cats)
lang_vowels <- vowel_cat %>%
filter(lang %in% langs)
ggplot(lang_vowels, aes(F2, F1, color = lang, label = vowel_category)) +
geom_text() +
stat_ellipse() +
scale_x_reverse() + scale_y_reverse() + labs(color = "Language")
}
#e.g. plot the vowel space overlap of /o/ in Czech and Russian
plot_overlap(c('Czech', 'Russian'), c('o'))
